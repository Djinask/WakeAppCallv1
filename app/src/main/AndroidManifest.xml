<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wakeappcallv1.app" >


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_md"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.example.wakeappcallv1.app.MainActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.wakeappcallv1.app.LoginActivity"
            android:label="@string/title_activity_login"
            android:windowSoftInputMode="adjustResize|stateVisible" >

        </activity>


        <activity
            android:name="com.example.wakeappcallv1.app.CalendarView"
            android:label="@string/title_activity_calendar_view" >
        </activity>

        <activity
            android:label="@string/app_name"
            android:name=".DashboardActivity" >

            <meta-data android:name="android.support.UI_OPTIONS"
                android:resource="@menu/menu_custom" />

            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--  Login Activity -->
        <activity
            android:label="Login Account"
            android:name=".LoginActivity2"></activity>

        <!--  Register Activity -->
        <activity
            android:name="com.example.wakeappcallv1.app.RegisterActivity"
            android:label="Register New Account" />
        <activity
            android:name="com.example.wakeappcallv1.app.Receiving_call"
            android:label="@string/title_activity_receiving_call" >
        </activity>
            android:name="com.example.wakeappcallv1.app.RegisterActivity"
            android:label="Register New Account" />
        <activity
            android:name="com.example.wakeappcallv1.app.ProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
    </application>

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />


</manifest>
